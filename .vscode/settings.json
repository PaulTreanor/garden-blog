{
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": [
    "source.fixAll",
    "source.organizeImports",
    "source.sortMembers"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.words": [
    "deallocates",
    "deallocating",
    "deallocation",
    "Firewalla",
    "frontmatter",
    "interruptible",
    "passwordless",
    "routable"
  ]
}
